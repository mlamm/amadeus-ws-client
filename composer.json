{
    "name": "flight/amadeus-service",
    "description": "A request service to amadeus",
    "type": "application",
    "license": "proprietary",
    "authors": [
        {
            "name": "Willi EÃŸer",
            "email": "willi.esser@unister.de"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Flight\\Service\\Amadeus\\": "./src"
        },
        "files": [
            "vendor/perftools/xhgui-collector/external/header.php",
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flight\\Service\\Amadeus\\Console\\": "./bin",
            "Flight\\Service\\Amadeus\\Tests\\": ["tests/unit"]
        }
    },
    "require": {
        "php": "~7.1",
        "silex/silex": "~2.0",
        "amabnl/amadeus-ws-client": "~1.4",
        "monolog/monolog": "~1.23",
        "flight/search-request-mapping": "^4.0",
        "doctrine/cache": "~1.7",
        "symfony/yaml": "~3.3",
        "jms/serializer": "^1.8",
        "flight/library-search-request-response-mapping": "3.2",
        "particle/validator": "^2.2",
        "flight/throwup-errors": "^0.1",
        "perftools/xhgui-collector": "^1.0",
        "alcaeus/mongo-php-adapter": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "~2.3",
        "mockery/mockery": "~0.9",
        "symfony/console": "~3.3",
        "symfony/finder": "~3.3",
        "symfony/filesystem": "~3.3",
        "zendframework/zend-code": "^3.2",
        "symfony/var-dumper": "^3.3",
        "justinrainbow/json-schema": "^5.2",
        "sorien/silex-pimple-dumper": "^2.0",
        "flow/jsonpath": "^0.3.4",
        "gamez/psr-testlogger": "^2.0",
        "digitaladapt/codeception-json-schema": "^0.1.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.unister.lan",
            "options": {
                "ssl": {
                    "verify_peer": false
                }
            }
        }
    ]
}
