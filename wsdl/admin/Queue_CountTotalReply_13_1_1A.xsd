      </xs:element>
      <xs:element name="status" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">contains the status of the count           to be worked, added, delayed, worked</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
