<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/QCSDRR_13_1_1A" xmlns="http://xml.amadeus.com/QCSDRR_13_1_1A" elementFormDefault="qualified">
  <xs:element name="Queue_CountTotalReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">response of standard display PNR queue counts</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errorReturn" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">any errors should be returned in this group</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="errorDefinition" type="ApplicationErrorInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">returns the error code</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="errorText" minOccurs="0" type="FreeTextInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">contains the text of the error</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="queueCountDisplay" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">return the targetted office, date and time of the display. Then date ranges Then the actual counts</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="localDateAndTime" type="StructuredDateTimeInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">to return the local date and time of the display</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="officeBeingDisplayed" type="AdditionalBusinessSourceInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">the office ID of the queuebank being displayed</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="dateRanges" minOccurs="0" maxOccurs="4" type="StructuredPeriodInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">only used when a single queue is to be displayed. There are 4 possible dates, the occurence of which range it is, is defined by the date range qualifier.   1st occurence - date range 1 2nd occurence - date range 2 3rd occurence - date range 3 4th occurence - date range 4</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="delayedCount" minOccurs="0" maxOccurs="2" type="QueueInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">used for the queue count total entry 1st occurence - delayed date 2nd occurnece - delayed time</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="standardQueueCountDisplay" maxOccurs="255">
                <xs:annotation>
                  <xs:documentation xml:lang="en">to transmit the counts using queue,category,date range and the different count types There are upto 60 queues each queue can have upto 255 categories each category upto 4 date ranges each date range 4 different count types</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="queueName" type="ReferenceInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">contains the unique queue name</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="queueNumber" type="QueueInformationTypeI_12831S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">contains the queue number</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="categoryAndCount" maxOccurs="1020">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">this group will contain the category and date range if needed and ALL the relevant counts There are upto 255 categories and each category can have 4 date ranges Each of these can have 4 different counts</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="categoryAndDateRange" type="SubQueueInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">contains the category and date range if required.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="queueCount" maxOccurs="4" type="QueueInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">contains the counts associated to a specfied queue, possible category and possible date range</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalBusinessSourceInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify additional originator and source information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="sourceType" type="SourceTypeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">the office we are targetting</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="originatorDetails" type="OriginatorIdentificationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">contains the office ID</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
