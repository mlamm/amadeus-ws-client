{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "result": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_itinerary": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "one-way",
                  "round-trip",
                  "open-jaw"
                ],
                "description": "requested type of offers"
              },
              "legs": {
                "type": "array",
                "items": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "provider": {
                        "type": "string",
                        "enum": [
                          "amadeus"
                        ],
                        "description": "requested gds"
                      },
                      "classification": {
                        "type": "string",
                        "enum": [
                          "scheduled",
                          "lowcost",
                          "charter"
                        ],
                        "description": "classification of leg information"
                      },
                      "carriers": {
                        "type": "object",
                        "properties": {
                          "main": {
                            "type": "object",
                            "properties": {
                              "iata": {
                                "type": "string",
                                "description": "IATA code of the main provider"
                              }
                            },
                            "required": [
                              "iata"
                            ]
                          },
                          "validating": {
                            "type": "object",
                            "properties": {
                              "iata": {
                                "type": "string",
                                "description": "IATA code of the validating carrier"
                              }
                            },
                            "required": [
                              "iata"
                            ]
                          }
                        },
                        "required": [
                          "main",
                          "validating"
                        ],
                        "description": "carrier information"
                      },
                      "duration": {
                        "type": "number",
                        "description": "flight duration of leg in seconds"
                      },
                      "nights": {
                        "type": "number",
                        "description": "amount of nights"
                      },
                      "segments": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "aircraft-type": {
                              "type": "string",
                              "description": "type of aircraft providing the flight"
                            },
                            "airports": {
                              "type": "object",
                              "properties": {
                                "arrival": {
                                  "type": "object",
                                  "properties": {
                                    "iata": {
                                      "type": "string",
                                      "description": "IATA code of arrival airport"
                                    }
                                  },
                                  "required": [
                                    "iata"
                                  ]
                                },
                                "departure": {
                                  "type": "object",
                                  "properties": {
                                    "iata": {
                                      "type": "string",
                                      "description": "IATA code of departure airport"
                                    }
                                  },
                                  "required": [
                                    "iata"
                                  ]
                                }
                              },
                              "required": [
                                "arrival",
                                "departure"
                              ]
                            },
                            "arrive-at": {
                              "type": "number",
                              "description": "arrival timestamp in local time"
                            },
                            "depart-at": {
                              "type": "number",
                              "description": "departure timestamp in local time"
                            },
                            "cabin-class": {
                              "type": "object",
                              "properties": {
                                "code": {
                                  "type": "string",
                                  "enum": [
                                    "C",
                                    "F",
                                    "M",
                                    "W",
                                    "Y"
                                  ],
                                  "description": "code for offered cabin class"
                                },
                                "name": {
                                  "type": "string",
                                  "description": "name of offered cabin class"
                                }
                              },
                              "required": [
                                "code",
                                "name"
                              ]
                            },
                            "flight-number": {
                              "type": "number",
                              "description": "number of the offered flight"
                            },
                            "carriers": {
                              "type": "object",
                              "properties": {
                                "marketing": {
                                  "type": "object",
                                  "properties": {
                                    "iata": {
                                      "type": "string",
                                      "description": "IATA code of the marketing carrier"
                                    }
                                  },
                                  "required": [
                                    "iata"
                                  ]
                                },
                                "operating": {
                                  "type": "object",
                                  "properties": {
                                    "iata": {
                                      "type": "string",
                                      "description": "IATA code of the operating carrier"
                                    },
                                    "name": {
                                      "type": "string",
                                      "description": "Name of the operating carrier.\nOnly given in some cases. The iata code will be an empty string in those cases."
                                    }
                                  },
                                  "required": [
                                    "iata"
                                  ]
                                }
                              },
                              "required": [
                                "marketing",
                                "operating"
                              ]
                            },
                            "remaining-seats": {
                              "type": [
                                "number",
                                "null"
                              ],
                              "description": "number of seats remaining for that flight"
                            },
                            "baggage-rules": {
                              "type": [
                                "object",
                                "null"
                              ],
                              "properties": {
                                "pieces": {
                                  "type": [
                                    "number",
                                    "null"
                                  ],
                                  "description": "number of baggage pieces included"
                                },
                                "weight": {
                                  "type": [
                                    "number",
                                    "null"
                                  ],
                                  "description": "value of baggage weight"
                                },
                                "unit": {
                                  "type": [
                                    "string",
                                    "null"
                                  ],
                                  "description": "unit of baggage weight"
                                }
                              }
                            },
                            "gds-information": {
                              "type": "object",
                              "properties": {
                                "res-book-desig-code": {
                                  "type": "string",
                                  "description": "the \"reservation booking designator\" code"
                                }
                              },
                              "required": [
                                "res-book-desig-code"
                              ]
                            },
                            "technical-stops": {
                              "type": [
                                "array",
                                "null"
                              ],
                              "items": {
                                "type": "object",
                                "properties": {
                                  "arrive-at": {
                                    "type": "number",
                                    "description": "arrival timestamp in local time"
                                  },
                                  "depart-at": {
                                    "type": "number",
                                    "description": "departure timestamp in local time"
                                  },
                                  "airport": {
                                    "type": "object",
                                    "properties": {
                                      "iata": {
                                        "type": "string",
                                        "description": "IATA code of the stopover airport"
                                      }
                                    },
                                    "required": [
                                      "iata"
                                    ]
                                  }
                                },
                                "required": [
                                  "arrive-at",
                                  "depart-at",
                                  "airport"
                                ]
                              }
                            }
                          },
                          "required": [
                            "aircraft-type",
                            "airports",
                            "arrive-at",
                            "depart-at",
                            "cabin-class",
                            "flight-number",
                            "carriers",
                            "gds-information"
                          ]
                        }
                      }
                    },
                    "required": [
                      "provider",
                      "classification",
                      "carriers",
                      "duration",
                      "nights",
                      "segments"
                    ]
                  }
                }
              }
            },
            "required": [
              "type",
              "legs"
            ]
          },
          "_calculation": {
            "type": "object",
            "properties": {
              "flight": {
                "type": "object",
                "properties": {
                  "total": {
                    "type": "number",
                    "description": "total price including fare and tax for all requested passengers"
                  },
                  "fare": {
                    "type": "object",
                    "properties": {
                      "total": {
                        "type": "number",
                        "description": "total fare for all requested passengers"
                      },
                      "passenger-types": {
                        "type": "object",
                        "properties": {
                          "adult": {
                            "type": "number",
                            "description": "fare price for each adult passenger"
                          },
                          "child": {
                            "type": "number",
                            "description": "fare price for each child passenger"
                          },
                          "infant": {
                            "type": "number",
                            "description": "fare price for each infant passenger"
                          }
                        },
                        "required": [
                          "adult",
                          "child",
                          "infant"
                        ]
                      }
                    },
                    "required": [
                      "total"
                    ]
                  },
                  "tax": {
                    "type": "object",
                    "properties": {
                      "total": {
                        "type": "number",
                        "description": "total fare for all requested passengers"
                      },
                      "passenger-types": {
                        "type": "object",
                        "properties": {
                          "adult": {
                            "type": "number",
                            "description": "tax for each adult passenger"
                          },
                          "child": {
                            "type": "number",
                            "description": "tax for each child passenger"
                          },
                          "infant": {
                            "type": "number",
                            "description": "tax for each infant passenger"
                          }
                        },
                        "required": [
                          "adult",
                          "child",
                          "infant"
                        ]
                      }
                    },
                    "required": [
                      "total"
                    ]
                  }
                },
                "required": [
                  "total"
                ]
              },
              "payment-methods": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "payment-fee": {
                      "type": "object",
                      "properties": {
                        "passenger-types": {
                          "type": "object",
                          "properties": {
                            "adult": {
                              "type": "number"
                            },
                            "child": {
                              "type": "number"
                            },
                            "infant": {
                              "type": "number"
                            }
                          },
                          "required": [
                            "adult",
                            "child",
                            "infant"
                          ]
                        },
                        "total": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "passenger-types",
                        "total"
                      ]
                    },
                    "name": {
                      "type": "string",
                      "enum": [
                        "_default"
                      ]
                    }
                  },
                  "required": [
                    "payment-fee",
                    "name"
                  ]
                }
              },
              "currency": {
                "type": "string",
                "description": "currency of the fare and tax values"
              }
            },
            "required": [
              "payment-methods",
              "currency"
            ]
          }
        },
        "required": [
          "_itinerary"
        ]
      }
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        }
      }
    }
  },
  "required": [
    "result"
  ]
}

