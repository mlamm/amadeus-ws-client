{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "session_id": {
      "type": "string",
      "description": "session id"
    },
    "sequence_number": {
      "type": "integer",
      "description": "sequence number"
    },
    "security_token": {
      "type": "string",
      "description": "security token"
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}